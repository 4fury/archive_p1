        <IsA>data</IsA>
        <Summary>Oxygen icon theme</Summary>
        <Description>oxygen-icon-theme is a high quality, photo-realistic icon theme (including both SVG and raster icons) initially created for KDE4.</Description>
        <Archive sha1sum="bc3d640258504c418e0445c25760b917f5f437cc" type="tarxz">mirrors://kde/stable/4.14.0/src/oxygen-icons-4.14.0.tar.xz</Archive>
    </Source>

    <Package>
        <Name>icon-theme-oxygen-png</Name>
        <Summary>Oxygen icon theme in PNG format</Summary>
        <Description>icon-theme-oxygen-png package contains Oxygen icon theme which is designed as default icon theme of KDE4 in PNG format.</Description>
        <RuntimeDependencies>
            <Dependency release="current">icon-theme-oxygen-index</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="data">/usr/share/icons/oxygen/8x8</Path>
            <Path fileType="data">/usr/share/icons/oxygen/16x16</Path>
            <Path fileType="data">/usr/share/icons/oxygen/22x22</Path>
            <Path fileType="data">/usr/share/icons/oxygen/32x32</Path>
            <Path fileType="data">/usr/share/icons/oxygen/48x48</Path>
            <Path fileType="data">/usr/share/icons/oxygen/64x64</Path>
            <Path fileType="data">/usr/share/icons/oxygen/128x128</Path>
            <Path fileType="data">/usr/share/icons/oxygen/256x256</Path>
        </Files>
        <AdditionalFiles>
            <AdditionalFile target="/usr/share/icons/oxygen/16x16/places/start-here.png">icons/16.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/22x22/places/start-here.png">icons/22.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/32x32/places/start-here.png">icons/32.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/48x48/places/start-here.png">icons/48.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/64x64/places/start-here.png">icons/64.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/128x128/places/start-here.png">icons/128.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/256x256/places/start-here.png">icons/256.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/16x16/places/start-here-kde.png">icons/16.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/22x22/places/start-here-kde.png">icons/22.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/32x32/places/start-here-kde.png">icons/32.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/48x48/places/start-here-kde.png">icons/48.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/64x64/places/start-here-kde.png">icons/64.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/128x128/places/start-here-kde.png">icons/128.png</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/256x256/places/start-here-kde.png">icons/256.png</AdditionalFile>
        </AdditionalFiles>
    </Package>

    <Package>
        <Name>icon-theme-oxygen-svg</Name>
        <Summary>Oxygen icon theme in SVG format</Summary>
        <Description>icon-theme-oxygen-svg package contains Oxygen icon theme which is designed as default icon theme of KDE4 in SVG format.</Description>
        <RuntimeDependencies>
            <Dependency release="current">icon-theme-oxygen-index</Dependency>
        </RuntimeDependencies>
        <Files>
            <Path fileType="data">/usr/share/icons/oxygen/scalable</Path>
        </Files>
    </Package>

    <Package>
        <Name>icon-theme-oxygen-index</Name>
        <Summary>Index file for Oxygen icon theme</Summary>
        <Description>icon-theme-oxygen-index package contains index.theme file for Oxygen icon theme which is a metadata file.</Description>
        <Files>
            <Path fileType="data">/usr/share/icons/oxygen/index.theme</Path>
            <Path fileType="doc">/usr/share/doc</Path>
        </Files>
        <AdditionalFiles>
            <AdditionalFile target="/usr/share/icons/oxygen/scalable/places/start-here-kde.svg">icons/scalable.svg</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/scalable/places/start-here-os.svg">icons/scalable.svg</AdditionalFile>
            <AdditionalFile target="/usr/share/icons/oxygen/scalable/places/start-here.svg">icons/scalable.svg</AdditionalFile>
        </AdditionalFiles>
    </Package>

